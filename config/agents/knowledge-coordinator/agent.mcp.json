{
  "mcpServers": {
    "context-store-http": {
      "type": "http",
      "url": "http://localhost:9501/mcp"
    },
    "agent-orchestrator-http": {
      "type": "http",
      "url": "${runner.orchestrator_mcp_url}",
      "headers": {
        "X-Agent-Session-Id": "${runtime.session_id}",
        "X-Agent-Tags": "internal"
      }
    }
  }
}
