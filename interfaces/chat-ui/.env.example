# Agent Coordinator API Configuration
# SSE endpoint is automatically derived as {API_URL}/sse/sessions
VITE_API_URL=http://localhost:8765

# API Authentication (required if auth is enabled on coordinator)
VITE_AGENT_ORCHESTRATOR_API_KEY=

# Auth0 Configuration (optional - enables login UI)
# When all three are set, login is required before using the app
VITE_AUTH0_DOMAIN=
VITE_AUTH0_CLIENT_ID=
VITE_AUTH0_AUDIENCE=

# Agent Configuration
# The agent blueprint to use for all chat sessions
VITE_AGENT_BLUEPRINT=knowledge-coordinator

# Optional: App title shown in the header
VITE_APP_TITLE=AI Assistant
